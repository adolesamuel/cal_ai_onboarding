{
    "editor.fontLigatures": true,
    "dart.maxLogLineLength": 10,
    "dart.lineLength": 120,
    "[dart]": {
      "editor.formatOnSave": true,
      "editor.formatOnType": true,
      "editor.rulers": [120],
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
      },

    }, 
  }